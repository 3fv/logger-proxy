{
  "name": "@3fv/logger-proxy",
  "version": "1.0.48",
  "description": "A logger proxy to enable hot swapping backends",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "init": "mkdir -p lib && cp package.json lib/",
    "build": "tsc -b tsconfig.json ${TSC_OPTS}",
    "build:watch": "cross-env TSC_OPTS=--watch yarn run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "./scripts/clean.sh",
    "postversion": "./scripts/release.sh $npm_package_version",
    "release": "yarn build  && yarn version --patch --non-interactive"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/3fv/logger-proxy.git"
  },
  "keywords": [
    "typescript"
  ],
  "files": [
    "**/*.d.ts",
    "**/*.js",
    "**/*.map",
    "package.json"
  ],
  "author": "Jonathan Glanz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/3fv/logger-proxy/issues"
  },
  "homepage": "https://github.com/3fv/logger-proxy#readme",
  "dependencies": {
    "@3fv/deferred": "^1.2.4",
    "@3fv/guard": "^1.4.26",
    "@3fv/prelude-ts": "^0.8.23",
    "bluebird": "^3.7.2",
    "chalk": "^4.1.2",
    "debug": "^4.3.2",
    "lodash": "^4.17.21"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/chalk": "^2.2.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.2",
    "@types/shelljs": "^0.8.9",
    "cross-env": "^7.0.3",
    "jest": "27.3.1",
    "lodash": "^4.17.21",
    "prettier": "^2.4.1",
    "semver": "^7.3.5",
    "source-map-support": "^0.5.20",
    "supports-color": "^9.0.2",
    "ts-jest": "27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
